<h1>Showing All Employees</h1>
<%= button_to 'Add Employee', new_employee_path,
					class: 'btn btn-primary', method: :get%>
<table class='table table-striped table-hover'>
	<%= render partial: 'table_header' %>
	<tbody>
		<% @employees.each do |employee| %>
			<%= render partial: 'employee', object: employee %>
		<% end %>
	</tbody>
</table>